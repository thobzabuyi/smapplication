<div class="row">
  <div class="col-md-12">
    
    <h1 class="text-center" style="color: #adb5bd;">Stock List</h1>
    <div class="row">
<%= will_paginate @items,  class: "ml-auto" %>
</div>
<table class="table table-striped table-dark">
  <thead class="thead-light">
    <tr>
      <th>Item name</th>
      <th>Brand name</th>
      <th>Quantity</th>
      <th>Cost price</th>
      <th>Sell price</th>
   
      
      <th colspan="3"></th>
    </tr>
  </thead>

 <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.item_name %></td>
        <td><%= item.brand_name %></td>
        <td><%= item.qualntity %></td>
        <td><%= number_to_currency_br(item.cost_price) %></td>
        <td><%= number_to_currency_br(item.sell_price) %></td>
        
       
      </tr>
    <% end %>
  </tbody>
</table>
</div>  
</div>
<div class="row">
<%= will_paginate @items,  class: "ml-auto" %>
</div>